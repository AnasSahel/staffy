{
  "name": "staffy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "pnpm db:migrate && next build",
    "start": "next start",
    "lint": "next lint",
    "auth:generate": "dotenvx run -f .env.local -- npx @better-auth/cli generate --y --output ./src/db/schema/auth.ts",
    "db:push": "dotenvx run -f .env.local -- drizzle-kit push",
    "db:generate": "dotenvx run -f .env.local -- drizzle-kit generate",
    "db:migrate": "dotenvx run -f .env.local -- drizzle-kit migrate",
    "db:studio": "dotenvx run -f .env.local -- drizzle-kit studio"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@t3-oss/env-nextjs": "^0.12.0",
    "better-auth": "^1.2.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "effect": "^3.14.8",
    "lucide-react": "^0.487.0",
    "next": "15.2.4",
    "pg": "^8.14.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.1.0",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.24.2",
    "zod-form-data": "^2.0.7"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.2.5",
    "@dotenvx/dotenvx": "^1.39.0",
    "@tailwindcss/postcss": "^4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^20",
    "@types/pg": "^8.11.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.30.6",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "tsx": "^4.19.3",
    "typescript": "^5"
  }
